<div class="p-4">
	<h4 class="fst-italic">Subjects :
	</h4>
	<ol class="list-unstyled mb-0">
		{%  for subject in subjects %}
			<li>
				<a class="text-decoration-none text-dark" href="{{ path('subject_show', { 'slug': subject.slug }) }}">{{ subject.name }}</a>
			</li>
		{% endfor %}
	</ol>
</div>
